{
  "name": "lexxusss/laravel-clickhouse",
  "description": "The Driver for Laravel Clickhouse ORM.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "clickhouse",
    "eloquent"
  ],
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/lexxusss/laravel-clickhouse"
    }
  ],
  "require": {
    "php": ">=7.1",
    "laravel/framework": "^5.5",
    "esazykin/clickhouse-builder": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "mockery/mockery": "^1.0",
    "fzaninotto/faker": "^1.7"
  },
  "autoload": {
    "psr-4": {
      "Lexxusss\\LaravelClickHouse\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lexxusss\\LaravelClickHouse\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --stop-on-failure tests/"
  }
}
